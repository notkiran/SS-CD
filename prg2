%{
#include "y.tab.h"
%}
%%
a {return A;}
b {return B;}
[\n] return '\n';
%%

%{
#include<stdio.h>
#include<stdlib.h>
%}
%token A B
%%
input:s'\n'{printf("Successful Grammer\n");exit(0);}
s:A s1 B | B
s1:;| A s1
%%
int yyerror()
{
printf("Error\n");
exit(0);
}
main()
{
printf("Enter A String \n");
yyparse();
}
